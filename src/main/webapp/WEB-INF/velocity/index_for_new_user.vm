<body class="fixed-sidebar full-height-layout gray-bg" style="overflow:hidden">
<div id="wrapper" style="background-color:transparent;overflow:visible">
    <!-- 头部开始 -->
    <div class="row head-bar" id="head-bar">
        <div class="col-lg-8 col-md-5 col-sm-7 col-xs-4 head-left">
            <img src="#springUrl('/images/logo.png')"/>
            <span class="hidden-xs">商户后台</span>
        </div>
        <div class="col-lg-4 col-md-7 col-sm-5 col-xs-8 head-right">
            <div class="loginout">
                <a href="#springUrl('/user/logout')">退出</a>
            </div>
            <div class="dropdown profile-element hidden-xs">
                <a data-toggle="dropdown" class="dropdown-toggle" href="#">
	    		<span class="clear">
			        <span class="block m-t-xs">
			            ${name}<br>
			        </span>
			        <span class="text-muted text-xs block">
                        <span>认证用户</span>
                        <b class="caret"></b>
			        </span>
	    		</span>
                </a>
                <ul class="dropdown-menu animated fadeInRight m-t-xs">
                    <li><a class="J_menuItem" href="/User_Account_editPassword.html">修改密码</a> </li>
                    <li class="divider"></li>
                    <li><a href="#springUrl('/user/logout')">安全退出</a> </li>
                </ul>
            </div>
            <div class="userimg hidden">
			<span><img alt="image" class="img-circle" src="#springUrl('/Public/Front/img/avatar.jpg')"
            ></span>
            </div>
            <div class="welcom hidden-xs">欢迎您</div>
        </div>
    </div>

    <!-- 头部结束 -->
    <!--左侧导航开始-->
    <nav class="navbar-default navbar-static-side" role="navigation" >
        <div class="nav-close"><i class="fa fa-times-circle"></i></div>
        <div class="sidebar-collapse">
            <ul class="nav" id="side-menu">
                <li>
                    <div class="logo-element visible-xs">MENU</div>
                </li>
                <li><a href="#"> <i class="fa fa-money"></i> <span class="nav-label">用户信息补充</span> </a>
                    <ul class="nav nav-second-level">
                        <li><a href="#springUrl('/common/index')" class="J_menuItem"><strong>信息补充</strong></a> </li>
                    </ul>
                </li>
            </ul>
        </div>
    </nav>

    <!--左侧导航结束-->
    <!--右侧部分开始-->

    <div id="page-wrapper" class="gray-bg dashbard-1" style="padding-right:0px;">
        <div class="row J_mainContent" id="content-main">
            <div class="navbar-header hidden"><a class="navbar-minimalize minimalize-styl-2 btn btn-primary " href="#"><i class="fa fa-bars"></i> </a></div>
            <iframe class="J_iframe" name="iframe0" width="100%" height="100%" src="#springUrl('/common/user_account_profile')" frameborder="0"
                    data-id="#springUrl('/common/user_account_profile')" seamless></iframe>
        </div>
    </div>
    <!--右侧部分结束-->

</div>
<!-- 全局js -->
<script src="#springUrl('/Public/Front/js/jquery.min.js')"></script>
<script src="#springUrl('/Public/Front/js/bootstrap.min.js')"></script>
